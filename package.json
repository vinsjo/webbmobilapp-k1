{
    "name": "webbmobilapp-ks1",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "reset-db": "node init-db.cjs --overwrite",
        "dev-server": "node init-db.cjs --overwrite false && npx json-server --watch db.json",
        "dev": "vite",
        "start": "npm run dev-server & npm run dev",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "format": "npx prettier --write src",
        "lint": "npx eslint src --ext .ts,.tsx,.js,.jsx",
        "prepare": "husky install"
    },
    "dependencies": {
        "@mantine/core": "^5.6.0",
        "@mantine/dates": "^5.6.0",
        "@mantine/hooks": "^5.6.0",
        "axios": "^1.1.3",
        "boring-avatars": "^1.7.0",
        "dayjs": "^1.11.5",
        "react": "latest",
        "react-dom": "latest",
        "react-icons": "^4.6.0",
        "react-router-dom": "^6.4.2",
        "x-is-type": "^1.2.0"
    },
    "devDependencies": {
        "@types/node": "latest",
        "@types/react": "latest",
        "@types/react-dom": "latest",
        "@typescript-eslint/eslint-plugin": "latest",
        "@typescript-eslint/parser": "latest",
        "@vitejs/plugin-react": "latest",
        "eslint": "latest",
        "eslint-plugin-react": "latest",
        "eslint-plugin-react-hooks": "^4.6.0",
        "husky": "^8.0.1",
        "json-server": "^0.17.0",
        "prettier": "latest",
        "pretty-quick": "latest",
        "typescript": ">=3.3.1 <4.9.0",
        "vite": "latest"
    }
}
